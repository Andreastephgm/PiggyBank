<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AgregarBanco">

    <EditText
        android:id="@+id/etdireccionin"
        android:layout_width="210dp"
        android:layout_height="44dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="28dp"
        android:ems="10"
        android:hint="Direccion"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ettipoin" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="117dp"
        android:layout_height="36dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="25dp"
        android:text="Direccion"
        android:textAlignment="center"
        android:textColor="#170101"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/etdireccionin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="156dp"
        android:layout_height="124dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="43dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/camara1" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="116dp"
        android:layout_height="27dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="25dp"
        android:text="Nombre"
        android:textAlignment="center"
        android:textColor="#070000"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/etnombrein"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <EditText
        android:id="@+id/etnombrein"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="188dp"
        android:layout_marginEnd="28dp"
        android:ems="10"
        android:hint="Nombre"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="125dp"
        android:layout_height="28dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="25dp"
        android:text="Sucursal"
        android:textAlignment="center"
        android:textColor="#0E0000"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/etsurcursalin"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <EditText
        android:id="@+id/etsurcursalin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="28dp"
        android:ems="10"
        android:hint="nombre Surcursal"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etnombrein" />

    <EditText
        android:id="@+id/ettipoin"
        android:layout_width="206dp"
        android:layout_height="44dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="28dp"
        android:ems="10"
        android:hint="Cajero,Banco,Surcursal"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etsurcursalin" />

    <EditText
        android:id="@+id/ethorarioin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="28dp"
        android:ems="10"
        android:hint="Horario"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etdireccionin" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="111dp"
        android:layout_height="30dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="25dp"
        android:text="Horario"
        android:textAlignment="center"
        android:textColor="#150101"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/ethorarioin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="112dp"
        android:layout_height="29dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="25dp"
        android:text="Tipo"
        android:textAlignment="center"
        android:textColor="#0E0000"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/ettipoin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Button
        android:id="@+id/btnAgregar"
        android:layout_width="205dp"
        android:layout_height="47dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="656dp"
        android:text="Agregar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnAgregarImagen"
        android:layout_width="187dp"
        android:layout_height="48dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="74dp"
        android:layout_marginEnd="31dp"
        android:text="Agregar Imagen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="108dp"
        android:layout_height="30dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="25dp"
        android:text="Latitud"
        android:textAlignment="center"
        android:textColor="#170101"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/etLatitudIn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="115dp"
        android:layout_height="26dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="25dp"
        android:text="Longitud "
        android:textAlignment="center"
        android:textColor="#0C0000"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/etLongitudIn"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <EditText
        android:id="@+id/etLatitudIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="28dp"
        android:ems="10"
        android:hint="Latitud"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ethorarioin" />

    <EditText
        android:id="@+id/etLongitudIn"
        android:layout_width="206dp"
        android:layout_height="42dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="43dp"
        android:ems="10"
        android:hint="Longitud"
        android:inputType="textPersonName"
        android:textAlignment="viewStart"
        android:textColor="#0B0000"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/btnAgregar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etLatitudIn"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnUbicacion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="287dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="9dp"
        android:text="UbicaciÃ³n"
        app:layout_constraintBottom_toTopOf="@+id/btnAgregar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etLongitudIn" />

</androidx.constraintlayout.widget.ConstraintLayout>